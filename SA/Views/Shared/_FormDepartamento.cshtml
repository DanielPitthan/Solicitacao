@model SA.Models.Departamento

<div class="container">
    <div class="row col-lg-12 col-xs-12">
        <div class="form-group">
            @Html.LabelFor(d => d.Departamentos, "Departamento")
            @Html.TextBoxFor(d => d.Departamentos, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(d => d.CentroCusto,"Centro de Custo")
            @Html.TextBoxFor(d=> d.CentroCusto, new { @class = "form-control" })
        </div>
    </div>
</div>